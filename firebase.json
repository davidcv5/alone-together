{
  "functions": {
    "predeploy": "npm --prefix functions run build",
    "source": "functions"
  },
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ {
      "source" : "/source",
      "destination" : "https://github.com/inlined/alone-together",
      "type" : 302
    } ],
    "rewrites": [{
      "source": "/rating",
      "function": "rating"
    }]
  }
}
